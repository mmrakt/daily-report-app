{
    "name": "next-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:analyze": "ANALYZE=true next dev",
        "build": "next build",
        "start": "next start",
        "export": "next build && next export -o dist",
        "lint": "eslint --ext .ts,.tsx ./src",
        "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
        "prettier:watch": "onchange '**/*.{ts,tsx,js}' -- prettier --config ./.prettierrc --write {{changed}}",
        "test": "jest --watch",
        "test:coverage": "jest --collect-coverage",
        "prisma:update": "prisma migrate dev && prisma generate",
        "prisma:seed": "ts-node ./prisma/seeds/seed.ts",
        "prisma:studio": "prisma studio"
    },
    "dependencies": {
        "@hookform/error-message": "^0.0.5",
        "@next-auth/prisma-adapter": "^1.0.4",
        "@next/bundle-analyzer": "^12.0.7",
        "@prisma/client": "^4.3.1",
        "@tailwindcss/forms": "^0.4.0-alpha.2",
        "@types/jest": "^26.0.19",
        "@types/node": "^14.14.22",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "autoprefixer": "^10.2.1",
        "dayjs": "^1.10.4",
        "dotenv": "^8.2.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.3",
        "minireset.css": "^0.0.6",
        "next": "^12.3.1",
        "next-auth": "^4.12.0",
        "nextjs-progressbar": "^0.0.13",
        "onchange": "^7.1.0",
        "postcss": "8.1.7",
        "prettier": "^2.2.1",
        "react": "^18.2.0",
        "react-calendar": "^3.3.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.35.0",
        "react-modal": "^3.12.1",
        "react-query": "^3.6.0",
        "react-responsive": "^9.0.0-beta.5",
        "react-router-dom": "^5.2.0",
        "react-test-renderer": "^18.2.0",
        "react-toastify": "^6.2.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.8.4",
        "uuid": "^8.3.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --ext .ts,.tsx ./src/"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "^18.0.21",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^29.1.2",
        "jest-dom": "^4.0.0",
        "jest-environment-jsdom": "^29.1.2",
        "msw": "^0.47.4",
        "prisma": "^4.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^9.1.1"
    },
    "moduleDirectories": [
        "node_modules",
        "src"
    ]
}
